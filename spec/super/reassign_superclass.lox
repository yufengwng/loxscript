class Base {
  method() {
    print("Base.method()");
  }
}

class Derived < Base {
  method() {
    super.method();
  }
}

class OtherBase {
  method() {
    print("OtherBase.method()");
  }
}

let derived = Derived();
derived.method(); #=> Base.method()
Base = OtherBase;
derived.method(); #=> Base.method()

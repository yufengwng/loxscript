print(none == none);  #=> true

print(true == true);  #=> true
print(true == false); #=> false

print(1 == 1); #=> true
print(1 == 2); #=> false

print("str" == "str"); #=> true
print("str" == "ing"); #=> false

print(none == false); #=> false
print(false == 0);    #=> false
print(0 == "0");      #=> false

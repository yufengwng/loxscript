print(none != none);  #=> false

print(true != true);  #=> false
print(true != false); #=> true

print(1 != 1); #=> false
print(1 != 2); #=> true

print("str" != "str"); #=> false
print("str" != "ing"); #=> true

print(none != false); #=> true
print(false != 0);    #=> true
print(0 != "0");      #=> true
